{
  "name": "sues-gpa-analyzer",
  "version": "1.0.0",
  "description": "上海工程技术大学GPA分析工具",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild -f -w keytar",
    "build:frontend": "vite build frontend --config vite.config.js",
    "build": "npm run rebuild && npm run build:frontend",
    "test": "vitest",
    "dev:frontend": "vite frontend --config vite.config.js",
    "dev:electron": "electron .",
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:electron\"",
    "package": "npm run build && electron-packager ./ SUES_GPA_Analyzer --win --arch=x64 --out ./out --asar=false --prune=true --overwrite",
    "make": "npm run package"
  },
  "keywords": [
    "gpa",
    "sues",
    "electron",
    "crawler"
  ],
  "author": "Folklore25",
  "license": "Apache-2.0",
  "devDependencies": {
    "@electron/packager": "^18.4.4",
    "@electron/rebuild": "^4.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@vitejs/plugin-react": "^5.0.1",
    "concurrently": "^9.2.1",
    "electron": "^37.3.0",
    "fs-extra": "^11.3.1",
    "rollup": "^4.49.0",
    "terser": "^5.43.1",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "electron-squirrel-startup": "^1.0.1",
    "jsdom": "^26.1.0",
    "keytar": "^7.9.0",
    "mathjs": "^11.12.0",
    "playwright": "^1.55.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
