{
  "name": "sues-gpa-analyzer",
  "version": "1.0.0",
  "description": "上海工程技术大学GPA分析工具",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "rebuild": "electron-rebuild -f -w keytar",
    "build:frontend": "vite build frontend --config vite.config.js",
    "build": "npm run rebuild && npm run build:frontend && electron-packager ./ SUES_GPA_Analyzer --win --arch=x64 --out ../outputs --asar=false --prune=true --overwrite --ignore=\"node_modules/(electron|playwright|@electron/.*)\" --ignore=\"(src|scripts)\"",
    "test": "vitest",
    "dev:frontend": "vite frontend --config vite.config.js",
    "dev:electron": "electron .",
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:electron\"",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [
    "gpa",
    "sues",
    "electron",
    "crawler"
  ],
  "author": "Folklore25",
  "license": "Apache-2.0",
  "build": {
    "appId": "com.sues.gpa-analyzer",
    "productName": "SUES GPA Analyzer",
    "icon": "logo.ico",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "src/**/*",
      "frontend/dist/**/*"
    ],
    "asar": false,
    "compression": "maximum",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}-${version}-setup.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^4.0.1",
    "@vitejs/plugin-react": "^5.0.1",
    "concurrently": "^9.2.1",
    "electron": "^37.3.0",
    "electron-packager": "^17.1.2",
    "terser": "^5.43.1",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "electron-squirrel-startup": "^1.0.1",
    "jsdom": "^26.1.0",
    "keytar": "^7.9.0",
    "mathjs": "^11.12.0",
    "playwright": "^1.55.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
